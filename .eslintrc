{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4, {"SwitchCase": 1}],
        "object-curly-spacing": 0,
        "class-methods-use-this": 0,
        "import/prefer-default-export": 0,
        "no-param-reassign": 0,
        "object-curly-newline": 0,
        "import/no-extraneous-dependencies": 0,
        "linebreak-style": 0,
        "comma-dangle": ["error", "always-multiline"]
    },
    "globals": {
        "document": true,
        "window": true
      }

}